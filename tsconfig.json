, "src/components/menubar/Menubar.vue", "src/components/menubar/AppMenu.vue", "src/components/menubar/Menubar.vue", "src/components/person/EditPerson.vue", "src/components/person/CreatePerson.vue", "src/components/person/ListPerson.vue"  ],
